{
  "version": "1.1.1",
  "description": "Get Push Notifications directly in your MetaMask wallet!",
  "proposedName": "Push",
  "repository": {
    "type": "git",
    "url": "https://github.com/ethereum-push-notification-service/push-protocol-snaps"
  },
  "source": {
    "shasum": "4/BEjM2xZqcgLmAVjRK2VQO6sqcqOAWhobmerBWJfaY=",
    "location": {
      "npm": {
        "filePath": "dist/bundle.js",
        "iconPath": "images/icon.svg",
        "packageName": "@pushprotocol/snap",
        "registry": "https://registry.npmjs.org/"
      }
    }
  },
  "initialPermissions": {
    "snap_dialog": {},
    "endowment:rpc": {
      "dapps": true,
      "snaps": false
    },
    "snap_notify": {},
    "endowment:cronjob": {
      "jobs": [
        {
          "expression": "* * * * *",
          "request": {
            "method": "fireCronjob"
          }
        },
        {
          "expression": "0 1 * * *",
          "request": {
            "method": "checkActivity"
          }
        }
      ]
    },
    "endowment:network-access": {},
    "snap_manageState": {},
    "endowment:ethereum-provider": {}
  },
  "manifestVersion": "0.1"
}
